#include <stdio.h>

int main()
{
    int N, a, b, c, x, y ,i;
    double s;
    scanf("%d", N);
    a=b=c=x=y=0;
    for (i = 0; i < N; i++) {
	scanf("%d%d", a, b);
	if (b >= 60)
	    c++;
	if (b >= 70)
	    c++;
	if (b >= 80)
	    c++;
	if (b >= 90)
	    c++;
	x += a*c;
	y += c;
    }
    return 0;
}
